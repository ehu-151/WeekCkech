<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!--メインの色-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorPrimaryLight">@color/colorPrimaryLight</item>
        <!--サブの色-->
        <item name="colorSub">@color/colorSub</item>
        <item name="colorSubLight">@color/colorSubLight</item>
        <!--直線を引くとき-->
        <item name="colorAccent">@color/colorAccent</item>
        <!--文字の色-->
        <item name="colorChar">@color/colorChar</item>
        <!--境界線の色-->
        <item name="colorBorder">@color/colorBorder</item>
    </style>

</resources>
